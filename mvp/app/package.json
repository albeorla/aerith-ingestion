{
  "name": "app",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "npm@9.8.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "npm run docker:up && next dev --turbo",
    "dev:clean": "npm run logs:clean && npm run docker:clean",
    "test": "NODE_ENV=test npm run docker:up && vitest run --no-file-parallelism",
    "test:watch": "NODE_ENV=test npm run docker:up && vitest --watch",
    "test:coverage": "NODE_ENV=test npm run docker:up && vitest run --coverage",
    "test:ui": "NODE_ENV=test npm run docker:up && vitest --ui",
    "test:debug": "NODE_ENV=test npm run docker:up && vitest run --no-file-parallelism --silent=false",
    "docker:up": "docker compose -f docker-compose.yml up -d",
    "docker:down": "docker compose -f docker-compose.yml down",
    "docker:build": "docker compose -f docker-compose.yml build",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "docker:clean": "docker compose -f docker-compose.yml down -v",
    "docker:logs": "docker compose -f docker-compose.yml logs -f",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "ts-node src/server/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "logs:clean": "rm -f dev-agent.log",
    "preview": "next build && next start",
    "start": "next start",
    "dev:agent": "(npm run format:write && npm run typecheck && npm run lint && npm run test) | tee -a dev-agent.log",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "drizzle-orm": "^0.33.0",
    "geist": "^1.3.0",
    "next": "^15.0.1",
    "next-auth": "5.0.0-beta.25",
    "pg": "^8.13.1",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.4",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  }
}
